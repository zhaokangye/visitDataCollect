<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kang.visit.module.Login.dao.UserMapper">

    <select id="getUser" resultType="com.kang.visit.module.Login.entity.User">
        select *
        from user
        where open_id=#{open_id}
    </select>
</mapper>
